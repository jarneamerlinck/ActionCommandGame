
@model PlayerShop
@{
    ViewData["Title"] = "Shop";


}
<partial name="PlayerHeaderPartial" model="@Model.Player"/>
<div class="container bg-gradient rounded">
    <div class="row">
        @foreach(var shopListItem in @Model.Items)
        {
            <partial name="ShopPartial" model="shopListItem"/>
        }
    </div>
</div>